
<div class="form-widget">
        {% for image in field.value %}
            <a href="#" class="ea-lightbox-thumbnail" data-ea-lightbox-content-selector="#ea-lightbox-{{ image.id }}-1">
                <img src="{{ vich_uploader_asset(image,"imageFile") }}" class="img-fluid">
            </a>

            <div id="ea-lightbox-{{ image.id }}-1" class="ea-lightbox">
                <img src="{{ vich_uploader_asset(image,"imageFile") }}">
            </div>
        {% endfor %}
</div>